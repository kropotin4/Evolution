<fx:root prefHeight="400.0" prefWidth="600.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="room_name_label_rp" text="Комната ххх" textFill="#426f1d" wrapText="true">
               <font>
                  <Font name="Arial Black" size="30.0" />
               </font>
            </Label>
            <VBox fx:id="connection_vbox" alignment="TOP_CENTER" fillWidth="false" prefWidth="100.0" style="-fx-border-width: 1; -fx-border-color: brown;">
               <children>
                  <Label text="Игроки">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <Button fx:id="ready_button_cp" disable="true" mnemonicParsing="false" text="Готов к игре" wrapText="true" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
